<view class="weui-cells__title title text-center font-size-16">输入你的设备IEMI号</view>
<view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_vcode">
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入文本" value="{{content}}" bindinput="inputCbf" />
        </view>
        <view class="weui-cell__ft">
            <button class="weui-vcode-btn add-iemi main-font-color" type="primary" bindtap="enter">确认</button>
        </view>
    </view>
</view>
